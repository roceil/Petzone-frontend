{
  "name": "petzone-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:console": "eslint . --ignore-pattern dist/ --rule 'no-console:[\"error\", { \"allow\": [\"warn\", \"error\", \"info\"] }]' --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/",
    "check": "npm run lint && npm run lint:console && npm run format && npm run test:unit",
    "vite": "vite"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.1",
    "@splinetool/loader": "^1.0.39",
    "@splinetool/runtime": "^1.0.39",
    "@vee-validate/i18n": "^4.12.5",
    "@vee-validate/rules": "^4.12.5",
    "@vee-validate/zod": "^4.12.5",
    "@yeger/vue-masonry-wall": "^5.0.6",
    "animejs": "^3.2.2",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "masonry-layout": "^4.2.2",
    "pinia": "^2.1.7",
    "swiper": "^11.0.6",
    "three": "^0.161.0",
    "vee-validate": "^4.12.5",
    "vue": "^3.3.11",
    "vue-masonry": "^0.16.0",
    "vue-masonry-wall": "^0.3.2",
    "vue-router": "^4.2.5",
    "vue3-marquee": "^4.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  }
}
